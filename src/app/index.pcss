@import "main/main.pcss";

html,
body,
#root {
    height: 100%;
    margin: 0;
    font-family: 'Open Sans', sans-serif;
}

body,
h1,
h2,
h3 {
    font-weight: 300;
}

a {
    text-decoration: none;
    color: white;
}

a:hover {
    color: #5aadbb;
}

.main-container {
    display: flex;
    flex-direction: column;
    min-height: 100%;
}
